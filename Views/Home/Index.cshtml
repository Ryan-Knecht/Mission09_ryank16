@{
}

@model List<Book>

<h1>The Bookstore</h1>

@foreach (Book b in Model)
{
    <h3>@b.Title</h3>

    <ul>
        <li>Author: @b.Author</li>
        <li>Publisher: @b.Publisher</li>
        <li>ISBN: @b.Isbn</li>
        <li>Classification: @b.Classification</li>
        <li>Number of Pages: @b.PageCount pgs</li>
        <li>Price: $@b.Price</li>

    </ul>

    <br />
}